[JOB]

job_type              = KILLER_RABBIT
job_runner_root       = /projects/v10/datacube/JR-STACK
app_path              = not_used
input_root            = not_used
product_root          = /g/data/v10/mdba
log_root              = /projects/v10/datacube/JR-STACK/log
#product_subdirectory  = 2000-01
#product_subdirectory  = bucket
product_subdirectory  =
job_name              = mdba
notification_email    = alex.ip@ga.gov.au
